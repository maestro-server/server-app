    "body-parser": "^1.17.1",
    "co": "^4.6.0",
    "construx": "^1.0.0",
    "construx-copier": "^1.0.0",
    "content-filter": "^1.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.12.2",
    "joi": "^10.5.2",
    "joi-objectid": "^2.0.0",
    "jwt-simple": "^0.5.1",
    "kraken-js": "^2.1.0",
    "lodash": "^4.17.4",
