    "gulp": "^3.9.1",
    "gulp-apidoc": "^0.2.6",
    "gulp-eslint": "^3.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-mocha": "^4.3.1",
    "mocha": "^3.4.1",
    "mocha-lcov-reporter": "^1.3.0",
