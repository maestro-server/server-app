{
  "name": "server-app",
  "version": "0.3.2",
  "description": "Server app - Maestro",
  "author": "Felipe Klerk Signorini &lt;felipeklerk@yahoo.com.br&gt;",
  "main": "server.js",
  "scripts": {
    "server": "nodemon -L server.js",
    "test": "mocha test/**/*js --timeout 5000 --reporter spec",
    "e2e": "gulp test_e2e",
    "unit": "gulp test_unit",
    "lint": "gulp eslint",
    "tdd": "gulp tdd",
    "package": "docker build -t 'server-maestro' .",
    "docs-inventory": "gulp apidocs --name inventory",
    "docs-identity": "gulp apidocs --name identity",
    "docs-reports": "gulp apidocs --name reports",
    "docs-adminer": "gulp apidocs --name adminer",
    "docs-playbook": "gulp apidocs --name playbooks",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly test/**/*js",
    "migrate": "node ./node_modules/mongodb-migrate -runmm -cfg .migration-config.js up",
    "create_migrate": "node ./node_modules/mongodb-migrate -runmm create",
    "down_migrate": "node ./node_modules/mongodb-migrate -runmm -cfg .migration-config.js down"
  },
  "dependencies": {
    "app-module-path": "~2.2.0",
    "aws-sdk": "^2.252.1",
    "axios": "^0.18.0",
    "bcrypt": "~2.0.0",
    "bluebird": "~3.5.0",
    "body-parser": "^1.18.3",
    "co": "~4.6.0",
    "construx": "~1.0.0",
    "construx-copier": "~1.0.0",
    "content-filter": "~1.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "joi": "~10.5.2",
    "joi-objectid": "~2.0.0",
    "jwt-simple": "~0.5.1",
    "kraken-js": "^2.2.0",
    "lodash": "~4.17.4",
    "mongodb-migrate": "~2.0.2",
    "mongorito": "~2.2.0",
    "nodemailer": "~4.0.1",
    "nodemailer-express-handlebars": "~2.0.0",
    "passport": "~0.3.2",
    "passport-jwt": "~2.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-deep-match": "~1.0.2",
    "chaid": "^1.0.2",
    "codacy-coverage": "~2.0.2",
    "codeclimate-test-reporter": "~0.5.0",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "gulp": "~3.9.1",
    "gulp-apidoc": "~0.2.6",
    "gulp-eslint": "^4.0.2",
    "gulp-load-plugins": "~1.5.0",
    "gulp-mocha": "^6.0.0",
    "minimist": "^1.2.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "~1.6.3",
    "nodemon": "^1.17.5",
    "sinon": "^6.0.0",
    "supertest": "^3.1.0"
  }
}
