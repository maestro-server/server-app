{
    "i18n": {
        "contentPath": "path:./locales",
        "fallback": "en-US"
    },
    "middleware": {
        "appsec": {
            "priority": 110,
            "module": {
                "name": "lusca",
                "arguments": [
                    {
                        "csrf": false
                    }
                ]
            }
        },
        "router": {
            "priority": 120,
            "module": {
                "arguments": [{"directory": "path:./core/routers"}]
            }
        },
        "router-architectures": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./architectures/routers"}]
            }
        },
        "router-profile": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./profile/routers"}]
            }
        },
        "router-projects": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./projects/routers"}]
            }
        },
        "router-applications": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./applications/routers"}]
            }
        },
        "router-teams": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./teams/routers"}]
            }
        },
        "router-inventory": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers"}]
            }
        },
        "router-servers": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./servers/routers"}]
            }
        },
        "router-clients": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./clients/routers"}]
            }
        },
        "router-adminer": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./adminer/routers"}]
            }
        },
        "json": {
            "priority": 30,
            "module": {
                "name": "body-parser",
                "method": "json"
            }
        },
        "url-encoded": {
            "priority": 35,
            "module": {
                "name": "body-parser",
                "method": "urlencoded",
                "arguments": [
                    {
                        "extended": true
                    }
                ]
            }
        },
        "filter": {
            "priority": 40,
            "module": {
                "name": "content-filter",
                "arguments": [{"dispatchToErrorHandler": true}]
            }
        },
        "cors": {
            "priority": 45,
            "module": "path:./core/middlewares/cors"
        },
        "tokenization": {
            "priority": 45,
            "module": "path:./core/middlewares/tokenization"
        },
        "notFound": {
            "priority": 130,
            "module": "path:./core/middlewares/404"
        },
        "validatorResponse": {
            "priority": 135,
            "module": "path:./core/middlewares/validatorResponse"
        },
        "notFoundResponse": {
            "priority": 135,
            "module": "path:./core/middlewares/notFoundResponse"
        },
        "permissionResponse": {
            "priority": 136,
            "module": "path:./core/middlewares/permissionResponse"
        },
        "conflictResponse": {
            "priority": 138,
            "module": "path:./core/middlewares/conflicResponse"
        },
        "errorResponse": {
            "priority": 140,
            "module": "path:./core/middlewares/errorCatcher"
        }
    }
}
