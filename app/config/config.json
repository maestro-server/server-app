{
    "i18n": {
        "contentPath": "path:./locales",
        "fallback": "en-US"
    },
    "middleware": {
      "session": {
        "enabled": false
      },
      "static": {
        "enabled": false
      },
      "cookieParser": {
        "enabled": false
      },
      "favicon": {
        "enabled": false
      },
      "filter": {
          "priority": 40,
          "module": {
              "name": "content-filter",
              "arguments": [{"dispatchToErrorHandler": true}]
          }
      },
      "cors": {
          "priority": 1,
          "module": "path:./core/middlewares/cors"
      },
      "tokenization": {
          "priority": 45,
          "module": "path:./core/middlewares/tokenization"
      },
      "appsec": {
          "priority": 110,
          "module": {
              "name": "lusca",
              "arguments": [
                  {
                      "csrf": false
                  }
              ]
          }
        },
        "router": {
            "priority": 120,
            "module": {
                "arguments": [{"directory": "path:./core/routers"}]
            }
        },
        "router-architectures": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./orchestration/routers/architectures"}]
            }
        },
        "router-profile": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./identity/routers/profile"}]
            }
        },
        "router-projects": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./identity/routers/projects"}]
            }
        },
        "router-applications": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers/applications"}]
            }
        },
        "router-teams": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./identity/routers/teams"}]
            }
        },
        "router-inventory": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers/system"}]
            }
        },
        "router-datacenters": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers/datacenters"}]
            }
        },
        "router-servers": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers/servers"}]
            }
        },
        "router-clients": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./inventory/routers/clients"}]
            }
        },
        "router-adminer": {
            "priority": 120,
            "module": {
                "name": "express-enrouten",
                "arguments": [{"directory": "path:./adminer/routers"}]
            }
        },
        "notFound": {
            "priority": 130,
            "module": "path:./core/middlewares/404"
        },
        "errorResponse": {
            "priority": 140,
            "module": "path:./core/middlewares/errorCatcher"
        }
    }
}
